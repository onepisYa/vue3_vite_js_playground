{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "checkJs": false,
    "allowJs":false
  },
  "exclude": ["node_modules", "dist"],
  "include":[ "src/**/*" , "src/**/*.vue"],
  "typeAcquisition":{
    "exclude":["/src/**/*.vue"]
  }
}
